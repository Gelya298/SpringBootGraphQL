#A Feed GraphGL schema
schema {
    query: Query
}
#Type Feed definicion
type Feed {
#    Identificator of the item
    id: ID!
#    Name of the RSS topic
    title: String!
#    Content of topic
    description: String!
    itemCount: Int
#    Date of publication
    pubdate: String!
#    URL of the Pictures
    image: String!
}

#Show the all items of a feed
extend type Query {
#    Just example of items set
    findAllFeeds: [Feed]!
#    Counter of items
    countFeeds: Long!
}
